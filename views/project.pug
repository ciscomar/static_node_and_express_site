extends layout.pug

block content 
  nav.grid-container.portfolio-breadcrumb
    a(href='/') ‚Üê Back
  article.grid-container.portfolio-intro.portfolio-project
    .grid-x
      .cell.small-6.medium-9.large-9

        h1=`${projectName}`

        p.lead.text-light= `${projectDes}`

      .cell.portfolio-meta.small-6.medium-2.large-3
        h6 Technologies

        ul
          each tech in projectTech
            li= tech

        a.button.expanded(href=`${projectLink}`) Live Demo
        a.button.expanded(href=`${projectGit}`) GitHub Repo
     
    each image in projectImages
      img.project-images.image(src=image)